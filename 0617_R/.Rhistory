insurance = read.csv("insurance.csv", header = T)
str(insurance)
data <- read.table(file = "data.txt", header = TRUE, sep = "")
plot(data, main="산점도", col="red")
hist(insurance$charges)
hist(log(insurance$charges))
ins_model <- lm(charges~., data=insurance)
summary(ins_model)
ins_model2 <- lm(log(charges)~., data=insurance)
summary(ins_model2)
insurance$age2 <- insurance$age^2
insurance$bmi30 <- ifelse(insurance$bmi >= 30, 1, 0)
ins_model3 <- lm(charges~age+age2+children+bmi+sex+bmi30*smoker+region, data=insurance)
